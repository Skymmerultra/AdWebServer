    <sql id="Base_Column_List">
        id,name,item_type,item_id,url,create_time,
        update_time,is_deleted
    </sql>

    <select id="selectByPrimaryKey" parameterType="java.lang.Long" resultMap="BaseResultMap">
        select
        <include refid="Base_Column_List" />
        from graph_info
        where  id = #{id} 
    </select>

    <delete id="deleteByPrimaryKey" parameterType="java.lang.Long">
        delete from graph_info
        where  id = #{id} 
    </delete>
    <insert id="insert" keyColumn="id" keyProperty="id" parameterType="com.ad.web.entity.Graph" useGeneratedKeys="true">
        insert into graph_info
        ( id,name,item_type,item_id,url,create_time,
        update_time,is_deleted)
        values (#{id},#{name},#{itemType},#{itemId},#{url},#{createTime},
        #{updateTime},#{isDeleted})
    </insert>
    <insert id="insertSelective" keyColumn="id" keyProperty="id" parameterType="com.ad.web.entity.Graph" useGeneratedKeys="true">
        insert into graph_info
        <trim prefix="(" suffix=")" suffixOverrides=",">
                <if test="id != null">id,</if>
                <if test="name != null">name,</if>
                <if test="itemType != null">item_type,</if>
                <if test="itemId != null">item_id,</if>
                <if test="url != null">url,</if>
                <if test="createTime != null">create_time,</if>
                <if test="updateTime != null">update_time,</if>
                <if test="isDeleted != null">is_deleted,</if>
        </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
                <if test="id != null">#{id},</if>
                <if test="name != null">#{name},</if>
                <if test="itemType != null">#{itemType},</if>
                <if test="itemId != null">#{itemId},</if>
                <if test="url != null">#{url},</if>
                <if test="createTime != null">#{createTime},</if>
                <if test="updateTime != null">#{updateTime},</if>
                <if test="isDeleted != null">#{isDeleted},</if>
        </trim>
    </insert>
    <update id="updateByPrimaryKeySelective" parameterType="com.ad.web.entity.Graph">
        update graph_info
        <set>
                <if test="name != null">
                    name = #{name},
                </if>
                <if test="itemType != null">
                    item_type = #{itemType},
                </if>
                <if test="itemId != null">
                    item_id = #{itemId},
                </if>
                <if test="url != null">
                    url = #{url},
                </if>
                <if test="createTime != null">
                    create_time = #{createTime},
                </if>
                <if test="updateTime != null">
                    update_time = #{updateTime},
                </if>
                <if test="isDeleted != null">
                    is_deleted = #{isDeleted},
                </if>
        </set>
        where   id = #{id} 
    </update>
    <update id="updateByPrimaryKey" parameterType="com.ad.web.entity.Graph">
        update graph_info
        set 
            name =  #{name},
            item_type =  #{itemType},
            item_id =  #{itemId},
            url =  #{url},
            create_time =  #{createTime},
